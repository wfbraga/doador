<div class="page-header">
  <%= link_to entradas_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_entrada_path(@entrada), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show entrada</h1>
</div>

<dl class="dl-horizontal">
  <dt>Data:</dt>
  <dd><%= @entrada.data %></dd>

  <dt>Valor:</dt>
  <dd><%= @entrada.valor %></dd>

  <dt>Categoria:</dt>
  <dd><%= @entrada.categoria %></dd>

  <dt>Descricao:</dt>
  <dd><%= @entrada.descricao %></dd>

</dl>
